            tab.addEventListener('click', function() {
                // アクティブタブの切り替え
                document.querySelectorAll('.nav-tab').forEach(t => t.classList.remove('active'));
                this.classList.add('active');

                // ページコンテンツの切り替え
                const pageId = 'page-' + this.dataset.page;
                document.querySelectorAll('.page-content').forEach(p => p.classList.remove('active'));
                document.getElementById(pageId).classList.add('active');

                // タイトルとページ番号の更新
                document.getElementById('current-page-title').textContent = this.dataset.title;
                document.getElementById('current-page-num').textContent = this.dataset.pagenum;
            });
        });

        // ブランド切り替え
        document.querySelectorAll('.brand-btn').forEach(btn => {
            btn.addEventListener('click', function() {
                // ボタンのスタイル切り替え
                document.querySelectorAll('.brand-btn').forEach(b => {
                    b.classList.remove('active');
                    b.style.background = 'white';
                    b.style.color = '#333';
                    b.style.borderColor = '#ccc';
                });
                this.classList.add('active');
                this.style.background = '#1B325A';
                this.style.color = 'white';
                this.style.borderColor = '#1B325A';

                // ブランドコンテンツの切り替え
                const brandId = 'brand-' + this.dataset.brand;
                document.querySelectorAll('.brand-content').forEach(c => {
                    c.style.display = 'none';
                    c.classList.remove('active');
                });
                document.getElementById(brandId).style.display = 'block';
                document.getElementById(brandId).classList.add('active');
            });
        });

        // Appendixサブナビゲーション切り替え
        document.querySelectorAll('.sub-nav-btn').forEach(btn => {
            btn.addEventListener('click', function() {
                // ボタンのアクティブ状態切り替え
                document.querySelectorAll('.sub-nav-btn').forEach(b => b.classList.remove('active'));
                this.classList.add('active');

                // セクションの切り替え
                const sectionId = 'appendix-' + this.dataset.section;
                document.querySelectorAll('.appendix-section').forEach(s => s.classList.remove('active'));
                document.getElementById(sectionId).classList.add('active');
            });
        });
